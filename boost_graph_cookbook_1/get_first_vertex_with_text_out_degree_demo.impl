#include <boost/test/unit_test.hpp>
#include "create_text_vertices_k2_graph.h"
#include "get_first_vertex_with_text_out_degree.h"

BOOST_AUTO_TEST_CASE(test_get_first_vertex_with_text_out_degree)
{
  const auto g = create_text_vertices_k2_graph();
  BOOST_CHECK(
    get_first_vertex_with_text_out_degree(
      "Me", g
    ) == 1
  );
  BOOST_CHECK(
    get_first_vertex_with_text_out_degree(
      "My computer", g
    ) == 1
  );
}
