#include "create_custom_vertices_petersen_graph.h"
#include <boost/test/unit_test.hpp>

BOOST_AUTO_TEST_CASE(test_create_custom_vertices_petersen_graph)
{
  const auto g = create_custom_vertices_petersen_graph();
